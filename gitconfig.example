[github]
	user = jlsuttles
  token = supersecret007
[alias]
  ci = commit
  co = checkout
  st = status
  pp = !git pull && git push
[core]
  editor = "mvim -f -c \"au VimLeave * !open -a Terminal\""
  legacyheaders = false
  excludesfile = ~/.gitignore
[push]
  default = current
[branch]
	autosetuprebase = always
[merge]
  summary = true
  keepBackup = false
[repack]
  usedeltabaseoffset = true
[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
